	pages = {61--83},
	file = {Henrich et al. - 2010 - The weirdest people in the world.pdf:/Users/nazliozum/Zotero/storage/ZLVVMRI3/Henrich et al. - 2010 - The weirdest people in the world.pdf:application/pdf}
}

